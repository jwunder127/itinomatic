<div class="NewUser-container">
  Welcome to the create user page
  <%= form_with(model: @user, url: users_path) do |f| %>
    <div>
      <%= f.label :first_name %>
      <%= f.text_field :first_name %>
    </div>
    <div>
      <%= f.label :last_name  %>
      <%= f.text_field :last_name %>
    </div>
    <div>
      <%= f.label :email %>
      <%= f.email_field :email %>
    </div>
    <%= f.submit "Submit" %>
  <% end %>
</div>